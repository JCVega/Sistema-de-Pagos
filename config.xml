<?xml version='1.0' encoding='utf-8'?>
<widget id="com.SistemaPagos.Chief" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Sistema Pagos/name>
    <description>
        Sistema para llevar un control de pagos ligero.
    </description>
    <author email="vega.garcia.juan.carlo@gmail.com" href="">
        Juan Carlos Vega Garc√≠a
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
